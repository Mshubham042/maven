<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ecommerce</servlet-name>
    <jsp-file>/ecommerce.jsp</jsp-file> <!-- Your JSP. Must begin with '/' -->
  </servlet>

  <servlet-mapping>
    <servlet-name>ecommerce</servlet-name>
    <url-pattern>/ecomm</url-pattern> <!-- Any URL you want here -->
  </servlet-mapping>


  <servlet>
    <servlet-name>admin</servlet-name>
    <jsp-file>/admin.jsp</jsp-file> <!-- Your JSP. Must begin with '/' -->
  </servlet>

  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/imadmin</url-pattern> <!-- Any URL you want here -->
  </servlet-mapping>

</web-app>
